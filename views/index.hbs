<div id="content">
  <header>
    <a class="hubTitle" href="/">Warframe Hub</a>
    <div id="settingsModal" class="hubSettings">Settings</div>
    <div class="hubDev" href="/">Development Build</div>
  </header>
  <nav>
    <p class="sectionTitle">Alerts</p>
    <ul class="sections">
      <li name="Alerts" class="active"><img src="img/navigation/alerts.svg"></li>
      <li name="Invasions"><img src="img/navigation/invasions.svg"></li>
      <li name="Fissures"><img src="img/navigation/fissures.svg"></li>
      <li name="Sortie"><img src="img/navigation/sortie.svg"></li>
      <li name="Ostron"><img src="img/navigation/ostron.svg"></li>
      <li name="Baro Ki'Teer"><img src="img/navigation/baro.svg"></li>
      <li name="Darvo Deals"><img src="img/navigation/darvo.svg"></li>
      <li name="Flash Sales"><img src="img/navigation/sales.svg"></li>
      <li name="Acolytes"><img src="img/navigation/acolytes.svg"></li>
      <li name="Event"><img src="img/navigation/event.svg"></li>
    </ul>
    {{> nav}}
  </nav>
  <div class="sectionContainer">
    <div id="alertList">
      <div id="alertbody">
        <span id="alerttitle">{{> loading}}</span>
      </div>
    </div>
  </div>
  <div class="timersContainer">
    <ul>
      <p class="sectionTitle">Timers</p>
      <li>Cetus Day/Night Cycle<div class="timerRight">Day - 3h 44m 55s</div></li>
      <li>Earth Day/Night Cycle<div class="timerRight">Day - 22m 11s</div></li>
      <li>Daily Reward Timer<div class="timerRight">15h 40m 50s</div></li>
      <p class="sectionTitle">Orbiter News</p>
      <li><a href="#" class="news">[3d] Beasts of the Sanctuary: Hotfix 22.20.3</a></li>
      <li><a href="#" class="news">[4d] Beasts of the Sanctuary: Update 22.20.0</a></li>
      <li><a href="#" class="news">[17d] Tenno Tunes Vol 3 Now Live!</a></li>
      <li><a href="#" class="news">[2M] TennoCon Digital Pack Available Now!</a></li>
      <li><a href="#" class="news">[2M] Zephyr Prime Access is Here!</a></li>
    </ul>
  </div>

		{{> footer}}
</div>
<!-- /.container -->

<!-- required js for page -->
<script src="js/utilities.js?v={{lookup sums 'js/utilities.js'}}"></script>
<script src="js/updaters.js?v={{lookup sums 'js/updaters.js'}}"></script>
<script src="js/main.js?v={{lookup sums 'js/main.js'}}"></script>
<script src="js/jquery.overlayScrollbars.min.js?v={{lookup sums 'js/jquery.overlayScrollbars.min.js'}}"></script>
<script src="js/ui.js?v={{lookup sums 'js/ui.js'}}"></script>


<script>
	/* overlayScrollbars */
  $('.sectionContainer').overlayScrollbars(
    {
      className: "os-theme-hub",
      overflowBehavior: {
        x: "hidden",
        y: "scroll"
      }
    });
</script>
